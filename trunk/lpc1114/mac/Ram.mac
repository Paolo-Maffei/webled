execUserReset()
{
  __message "------- execute UserReset -----------";
  __writeMemory32(0x00000001, 0x40048000, "Memory"); // SYSMEMREMAP = 1
}

execUserPreload()
{
  __message "------- execute UserPreLoad -----------";
  __writeMemory32(0x00000001, 0x40048000, "Memory"); // SYSMEMREMAP = 1
}
